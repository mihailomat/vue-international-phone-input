{
  "name": "vue-phone-input",
  "version": "0.0.1",
  "private": false,
  "description": "Internation phone number input Vue component.",
  "main": "dist/vue-phone-input.ssr.js",
  "browser": "dist/vue-phone-input.esm.js",
  "module": "dist/vue-phone-input.esm.js",
  "unpkg": "dist/vue-phone-input.min.js",
  "sideEffects": false,
  "scripts": {
    "serve": "vue serve src/PhoneInput.vue",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihailomat/vue-phone-input.git"
  },
  "author": "Mihailo Matijevic (mihailomat)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mihailomat/vue-phone-input/issues"
  },
  "homepage": "https://github.com/mihailomat/vue-phone-input#readme",
  "dependencies": {},
  "devDependencies": {
    "@vue/cli": "^4.5.11",
    "@vue/cli-service-global": "^4.5.11",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
